{
    "name": "ESP32-WiFi",
    "keywords": "FEmbed, esp32, arduino, wifi",
    "description": "arduino-esp32 WIFI library",
    "version": "1.0",
    "frameworks": "espidf",
    "platforms": "espressif32",
    "build": {
        "flags": [
            "-I src",
            "-DUSE_ESP32_WIFI=1"
        ],
        "srcFilter":[
            "+<*.c>",
            "+<*.cpp>",
            "+<*.h>"
        ]
    },
    "dependencies": [
        {
            "name": "FEmbed-Arduino",
            "version": ">=0.1.0",
            "platforms": ["espressif32"]
        }
    ]
}